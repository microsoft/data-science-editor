    "@blockly/theme-dark": "^2.0.6",
    "@blockly/theme-modern": "^2.1.24",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mdx-js/react": "^2.2.1",
    "@microsoft/applicationinsights-web": "^2.8.9",
